{
  "name": "tax-measures-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "xo",
    "publish-search": "export $(cat .env) && node ./tasks/publish-search"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@turf/bbox": "^6.0.1",
    "algoliasearch": "^4.2.0",
    "apexcharts": "^3.19.0",
    "bootstrap": "^4.4.1",
    "cross-fetch": "^3.0.4",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "next": "9.4.0",
    "next-seo": "^4.5.0",
    "react": "16.13.1",
    "react-apexcharts": "^1.3.7",
    "react-bootstrap": "^1.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-csv": "^2.0.3",
    "react-dom": "16.13.1",
    "react-https-redirect": "^1.1.0",
    "react-map-gl": "^5.2.5",
    "react-select": "^3.1.0",
    "sass": "^1.26.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-xo-nextjs": "^1.6.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "xo": "^0.30.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z]"
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
